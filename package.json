{
  "name": "@airhead/typeorm-seeder",
  "version": "0.0.0-dev",
  "description": "Decorative seeder for TypeORM",
  "license": "MIT",
  "author": {
    "name": "Joakim Bugge",
    "email": "joakim.bugge@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joakimbugge/typeorm-seeder.git"
  },
  "bugs": {
    "url": "https://github.com/joakimbugge/typeorm-seeder/issues",
    "email": "joakim.bugge@gmail.com"
  },
  "keywords": [
    "typeorm",
    "seeder"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "start": "ts-node playground/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "rollup -c rollup.config.js",
    "pack": "npm run build && cd dist && npm pack",
    "lint": "eslint .",
    "lint:commit": "commitlint",
    "lint:types": "tsc --noEmit",
    "prettify": "prettier --write src",
    "postversion": "cp -r package.json ..",
    "prepare": "is-ci || husky install"
  },
  "peerDependencies": {
    "typeorm": ">=0.2.x"
  },
  "dependencies": {
    "@hapi/topo": "^5.1.0",
    "app-root-path": "^3.0.0",
    "glob": "^7.1.7",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.1.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@ngneat/falso": "^5.4.1",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/git": "^10.0.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/app-root-path": "^1.2.4",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "better-sqlite3": "^7.5.0",
    "commitlint": "^16.1.0",
    "conventional-changelog-conventionalcommits": "^4.6.3",
    "coveralls": "^3.1.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.1",
    "pg": "^8.7.1",
    "prettier": "^2.5.1",
    "rollup": "^2.66.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "semantic-release": "^19.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tsc --noEmit",
      "eslint",
      "prettier --write"
    ],
    "src/**/*.json": [
      "prettier --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
